<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".logIn_EnterPassword">

    <Button
        android:id="@+id/btnSignInGo"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/custom_circle_button"
        android:text="Go"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSignInPassword"
        app:layout_constraintVertical_bias="0.100000024" />

    <EditText
        android:id="@+id/btnSignInPassword"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:layout_marginTop="288dp"
        android:background="@drawable/custom_edit_text"
        android:drawableLeft="@drawable/password_lock"
        android:ems="10"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Password"
        android:inputType="textPassword"
        android:paddingLeft="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>